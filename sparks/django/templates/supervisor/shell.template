[program:%(program)s]
command=nice -n -7 ionice -c 2 -n 2 %(command_pre_args)s %(user_home)s/.virtualenvs/%(virtualenv)s/bin/python %(root)s/manage.py shell_ipynb %(command_post_args)s
directory=%(root)s
user=%(user)s
autostart=true
autorestart=true
redirect_stderr=true
priority=991
stopsignal=KILL
%(environment)s
